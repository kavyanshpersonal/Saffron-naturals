    <script>
        // Navigation Functions
        function toggleSidebar() {
            const sidebar = document.getElementById('sidebar');
            sidebar.classList.toggle('-translate-x-full');
        }

        function showDashboardSection(sectionName) {
            // Hide all sections
            const sections = document.querySelectorAll('.dashboard-section');
            sections.forEach(section => {
                section.classList.remove('active');
            });

            // Remove active class from all nav items
            const navItems = document.querySelectorAll('.nav-item');
            navItems.forEach(item => {
                item.classList.remove('active');
            });

            // Show selected section
            const targetSection = document.getElementById(sectionName + 'Section');
            if (targetSection) {
                targetSection.classList.add('active');
            }

            // Add active class to current nav item
            const navButtons = document.querySelectorAll('.nav-item');
            navButtons.forEach(button => {
                if (button.textContent.toLowerCase().includes(sectionName) || 
                    (sectionName === 'overview' && button.textContent.includes('Business Dashboard')) ||
                    (sectionName === 'intelligence' && button.textContent.includes('Business Intelligence')) ||
                    (sectionName === 'logistics' && button.textContent.includes('Live Logistics')) ||
                    (sectionName === 'farm' && button.textContent.includes('Farm Monitoring')) ||
                    (sectionName === 'blockchain' && button.textContent.includes('Blockchain Ledger')) ||
                    (sectionName === 'partnership' && button.textContent.includes('Partnership Tools')) ||
                    (sectionName === 'academy' && button.textContent.includes('Academy Access')) ||
                    (sectionName === 'orders' && button.textContent.includes('Order History')) ||
                    (sectionName === 'profile' && button.textContent.includes('Account Profile'))) {
                    button.classList.add('active');
                }
            });

            // Close sidebar on mobile after navigation
            if (window.innerWidth < 1024) {
                document.getElementById('sidebar').classList.add('-translate-x-full');
            }
        }

        // Business Intelligence Functions
        function exportIntelligenceReport() {
            alert('üìä BUSINESS INTELLIGENCE REPORT\n\nGenerating comprehensive analytics report...\n\nReport includes:\n‚Ä¢ Purchase patterns analysis\n‚Ä¢ Seasonal trend insights\n‚Ä¢ Cost optimization recommendations\n‚Ä¢ Predictive analytics\n‚Ä¢ Performance benchmarks\n\nReport will be emailed to ahmed@emiratesfresh.ae');
        }

        // Logistics Functions
        function refreshTracking() {
            alert('üîÑ TRACKING REFRESH\n\nUpdating real-time tracking data...\n\n‚úÖ All shipment locations updated\n‚úÖ ETA calculations refreshed\n‚úÖ Status notifications synchronized\n\nLast updated: Just now');
        }

        function trackNewShipment() {
            const trackingId = prompt('üìç TRACK NEW SHIPMENT\n\nEnter tracking ID or order number:');
            if (trackingId) {
                alert(`üöö TRACKING: ${trackingId}\n\nSearching for shipment...\n\n‚úÖ Shipment found!\nüìç Current location: Mumbai Port\nüö¢ Status: Loading in progress\n‚è∞ ETA: 3 days\n\nFull tracking details loaded!`);
            }
        }

        // Farm Monitoring Functions
